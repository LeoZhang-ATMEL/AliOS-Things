{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "alios-studio: Make",
      "type": "shell",
      "command": "aos",
      "args": [
        "make",
        "mqttapp@atsame54-xpro",
        "sal=1",
        "module=wifi.mk3060",
        "press_test=1",
        "BUILD_TYPE=debug",
        "vcall=rhino",
        "_test=certificate"
      ],
      "presentation": {
        "panel": "dedicated"
      }
    },
    {
      "label": "alios-studio: Erase",
      "type": "shell",
      "command": "D:/Program Files (x86)/Atmel/Studio/7.0/atbackend/atprogram.exe",
      "args": [
        "--tool",
        "edbg",
        "--interface",
        "SWD",
        "--device",
        "ATSAME54P20A",
        "erase"
      ],
      "presentation": {
        "panel": "dedicated"
      }
    },
    {
      "label": "alios-studio: Upload",
      "type": "shell",
      "command": "D:/Program Files (x86)/Atmel/Studio/7.0/atbackend/atprogram.exe",
      "args": [
        "--tool",
        "edbg",
        "--interface",
        "SWD",
        "--device",
        "ATSAME54P20A",
        "program",
        "-f",
        "${workspaceRoot}/out/mqttapp@atsame54-xpro/binary/mqttapp@atsame54-xpro.hex"
      ],
      "presentation": {
        "panel": "dedicated"
      }
    },
    {
      "label": "alios-studio: Serial Monitor",
      "type": "shell",
      "command": "aos",
      "args": [
        "monitor",
        "COM3",
        "115200"
      ],
      "presentation": {
        "panel": "dedicated"
      }
    },
    {
      "label": "alios-studio: Clean",
      "type": "shell",
      "command": "aos",
      "args": [
        "make",
        "clean"
      ],
      "presentation": {
        "panel": "dedicated"
      }
    }
  ]
}
